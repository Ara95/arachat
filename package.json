    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.16.2",
    "express-session": "^1.15.5",
    "istanbul": "^0.4.5",
    "mongo-mock": "^3.1.0",
    "mongodb": "^2.2.33",
    "mongoose": "^4.11.5",
    "nyc": "^11.4.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pug": "^2.0.0-rc.4",
